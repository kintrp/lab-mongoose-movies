<h1>Movies</h1>
{{#each movies}}
    title: <h3>{{this.title}}</h2>
    plot: <h3>{{this.plot}}</h3>
    {{#each this.cast}}
    director: <h3>{{ name }}</h3>   
    {{/each}}
    <a href="/movies/{{ this._id }}/edit">Edit</a>
{{/each}}
<br>

    <a href="/movies/new" class="buttonAdd">Add new movie</a> <br>

    <a href="http://localhost:3000" class="button">Back to celebrities</a>


